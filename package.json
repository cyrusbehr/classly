{
  "name": "generator-simple-redux",
  "version": "1.0.0",
  "description": "My simple redux application.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --profile --colors",
    "build": "npm run clean && npm run build:webpack --progress --profile --colors",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "start": "node devServer.js",
    "webpack": "webpack --config webpack.config.dev.js",
    "webpack-dev": "webpack --watch --config webpack.config.dev.js",
    "frontend": "webpack -d --watch --config webpack.config.dev.js",
    "backend": "node devServer.js",
    "lint": "eslint src",
    "postinstall": "npm run build:webpack"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          "babel-plugin-transform-react-remove-prop-types"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsaiDavid/simple-redux-boilerplate.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/tsaiDavid/simple-redux-boilerplate",
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "axios": "^0.16.2",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.5.0",
    "connected-react-router": "^4.2.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "date-now": "^1.0.1",
    "dateformat": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.16.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "express-validator": "^3.2.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.24.1",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "material-ui": "^0.19.0",
    "mongoose": "^5.11.9",
    "node-sass": "^4.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rc-collapse": "^1.7.6",
    "react": "^0.14.9",
    "react-alert": "^2.2.0",
    "react-autocomplete": "^1.5.10",
    "react-bootstrap": "^0.31.2",
    "react-collapse": "^4.0.3",
    "react-dom": "^0.14.3",
    "react-modal": "^2.2.4",
    "react-motion": "^0.5.0",
    "react-redux": "^4.1.1",
    "react-router-dom": "^4.1.2",
    "react-tooltip": "^3.3.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.3.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.13.0",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.9",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-hot-middleware": "^2.6.0"
  }
}
